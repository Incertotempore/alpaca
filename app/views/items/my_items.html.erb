<h2>Displays all the items the current user is offering for renting</h2>
<% @items.each do |item|                                          %>
<%   if item.user == current_user                                 %>
     <p><%= link_to item.name, item_path(item) %></p>
<%= link_to "Edit Item", edit_item_path(item), class: "btn btn-success"  %>
<%   end                                                          %>
<% end                                                            %>
